{
  "kind": "singleType",
  "collectionName": "homepages",
  "info": {
    "singularName": "homepage",
    "pluralName": "homepages",
    "displayName": "Homepage",
    "name": "homepage",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "products": {
      "type": "component",
      "repeatable": true,
      "component": "product.product-component"
    },
    "homepageFeatureTabs": {
      "type": "component",
      "repeatable": false,
      "component": "homepage.homepage-feature-tabs"
    },
    "description": {
      "type": "richtext"
    },
    "storyTabs": {
      "type": "component",
      "repeatable": false,
      "component": "sections.story-tabs"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "suptitle": {
      "type": "richtext"
    },
    "template_category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::template.template-category"
    },
    "title": {
      "type": "string"
    },
    "sections": {
      "type": "dynamiczone",
      "components": [
        "sections.items-section",
        "sections.compare-info-section",
        "sections.templates-section",
        "sections.testimonial-section",
        "sections.top-section",
        "sections.tour-section",
        "sections.faq-section",
        "sections.product-list-section",
        "sections.benefits",
        "sections.cta-section",
        "sections.story-section",
        "sections.stars-reviews-section",
        "sections.story-tabs",
        "homepage.homepage-feature-tabs",
        "sections.table-section"
      ]
    }
  }
}
